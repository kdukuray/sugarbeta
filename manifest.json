{
    "manifest_version": 3,
    "name": "SugarBeta",
    "version": "0.1",
    "icons": {
        "16": "/sugarcube-small.png",
        "48": "/sugarcube-small.png"
    },
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ] 
}